<openerp>
    <data>
        <!-- Purchase Request -->
        <record id="base_sequence_purchase_request" model="base.sequence_configurator">
            <field name="model_id" ref="purchase_request.model_purchase_request"/>
            <field name="sequence_field_id" search="[('model_id.model','=','purchase.request'),('name','=','name')]"/>
            <field name="fallback_sequence_id" ref="purchase_request.seq_purchase_request"/>
        </record>

        <record id="base_sequence_purchase_request_line" model="base.sequence_configurator_line">
            <field name="generator_id" ref="base_sequence_purchase_request"/>
            <field name="sequence" eval="1"/>
            <field name="sequence_computation_code">result = document.order_type_id.sequence_id</field>
        </record>
    </data>
</openerp>
